<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- NavigationBar -->
    <declare-styleable name="NavigationBar">
        <attr name="NavigationBar_background" format="reference"></attr>
        <attr name="NavigationBar_title" format="reference|string"></attr>
        <attr name="NavigationBar_leftBackground" format="reference"></attr>
        <attr name="NavigationBar_textSize" format="dimension"></attr>
        <attr name="NavigationBar_textColor" format="reference|color"></attr>
    </declare-styleable>

    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color"></attr>
    </declare-styleable>

</resources>
